    color: #0F2434;
    background-color: #FBFAF5;
}

/* Alternate row coloring */
#table_candidat::item:alternate {
    background-color: #F0F0F0;
}

/* Hover effect on rows */
#table_candidat::item:hover {
    background-color: #E0E0E0;
    color: #0F2434;
}

/* Selected row styling */
#table_candidat::item:selected {
    background-color: #0F2434;
    color: #FBFAF5;
}

/* Ensure the table view itself has proper borders */
#table_candidat QTableView {
    gridline-color: #0F2434;
    border: 2px solid #0F2434;
}

/* Corner button styling */
#table_candidat QTableCornerButton::section {
    background-color: #0F2434;
    border: 1px solid #0F2434;
}</string>

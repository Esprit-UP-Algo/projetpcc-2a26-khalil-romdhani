    background-color: #E0E0E0;
    color: #0F2434;
}

/* Selected row styling */
#tableWidget::item:selected {
    background-color: #0F2434;
    color: #FBFAF5;
}

/* Ensure the table view itself has proper borders */
#tableWidget QTableView {
    gridline-color: #0F2434;
    border: 2px solid #0F2434;
}

/* Corner button styling */
#tableWidget QTableCornerButton::section {
    background-color: #0F2434;
    border: 1px solid #0F2434;
}</string>

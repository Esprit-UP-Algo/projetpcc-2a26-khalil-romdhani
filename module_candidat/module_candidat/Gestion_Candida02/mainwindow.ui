       <property name="styleSheet">
        <string notr="true">/* Main table styling */
#tableWidget {
    background-color: #FBFAF5;
    border-radius: 8px;
    border: 2px solid #0F2434;
    gridline-color: #0F2434;
    alternate-background-color: #E8E8E8;
}

/* Table header styling - make columns visible */
#tableWidget QHeaderView::section {
    background-color: #0F2434;
    color: #FBFAF5;
    padding: 8px;
    border: 1px solid #1D9E3A;
    font-weight: bold;
    font-size: 12px;
}

/* Table items/cells styling */
#tableWidget::item {
    border: 1px solid #0F2434;
    padding: 8px;
    color: #0F2434;
}

/* Make sure text is visible in cells */
#tableWidget::item {
    color: #0F2434;
    background-color: #FBFAF5;
}

/* Alternate row coloring */
#tableWidget::item:alternate {
    background-color: #F0F0F0;
}

/* Hover effect on rows */
#tableWidget::item:hover {
    background-color: #E0E0E0;
    color: #0F2434;
}

/* Selected row styling */
#tableWidget::item:selected {
    background-color: #0F2434;
    color: #FBFAF5;
}

/* Ensure the table view itself has proper borders */
